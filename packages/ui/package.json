{
  "name": "@thorbis/ui",
  "version": "0.1.0",
  "description": "Thorbis UI Components - Reusable React components following Thorbis design principles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./overlay-free/*": {
      "import": "./dist/overlay-free/*.js",
      "require": "./dist/overlay-free/*.js",
      "types": "./dist/overlay-free/*.d.ts"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*.js",
      "require": "./dist/hooks/*.js",
      "types": "./dist/hooks/*.d.ts"
    }
  },
  "files": [
    "dist",
    "components",
    "overlay-free",
    "hooks"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@thorbis/design": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.263.1",
    "react-day-picker": "^8.10.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.47.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.0"
  }
}